  <dtChecklist>
    <ChecklistKey>25</ChecklistKey>
    <ScriptName>Backup History</ScriptName>
    <CheckDesc>All databases backup history</CheckDesc>
    <LowValue>1</LowValue>
    <HighValue>1</HighValue>
    <IsWithinRangeGood>true</IsWithinRangeGood>
