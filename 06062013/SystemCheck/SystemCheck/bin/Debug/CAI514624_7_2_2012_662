			CONVERT(DECIMAL(18,3),(@BatchRequests - @SqlCompilations) / @BatchRequests),
			'Batch Requests = ' + CONVERT(VARCHAR(25),@BatchRequests)
			+ CHAR(10) + CHAR(13) +
			'SQL Compilations: ' + CONVERT(VARCHAR(25), @SqlCompilations)
